<script>
// Toggle Bio Function
function toggleBio(btn) {
    const container = btn.previousElementSibling;
    const isExpanded = container.classList.contains('bio-expanded');
    
    if (isExpanded) {
        container.classList.remove('bio-expanded');
        btn.innerText = 'Read More';
        btn.closest('article').scrollIntoView({ behavior: 'smooth', block: 'nearest' });
    } else {
        container.classList.add('bio-expanded');
        btn.innerText = 'Show Less';
    }
}

// Check visibility of Read More buttons based on line height
function checkBioTruncation() {
    const bios = document.querySelectorAll('.bio-text');
    bios.forEach(bio => {
        const btn = bio.nextElementSibling;
        if (!btn || !btn.classList.contains('read-more-btn')) return;

        const visibleHeight = bio.clientHeight;
        const fullHeight = bio.scrollHeight;

        if (fullHeight <= visibleHeight + 5) {
            btn.style.display = 'none';
        } else {
            btn.style.display = 'inline-block';
        }
    });
}

document.addEventListener('DOMContentLoaded', function() {
    setTimeout(checkBioTruncation, 200); 
    lucide.createIcons();
    
    // Navbar Logic
    const navToggle = document.querySelector('.navbar-toggler, .hamburger, .kjb-navbar-toggle, [data-toggle="collapse"]')
    const navMenu = document.querySelector('.navbar-collapse, .nav-menu, .kjb-navbar-collapse')

    if (navToggle && navMenu) {
        navToggle.addEventListener('click', function(e) {
            e.preventDefault()
            const isHidden = navMenu.classList.contains('hidden') || 
                            window.getComputedStyle(navMenu).display === 'none' ||
                            !navMenu.classList.contains('show')

            if (isHidden) {
                navMenu.classList.remove('hidden'); navMenu.classList.add('show');
                navMenu.style.display = 'block'; navToggle.classList.remove('collapsed')
            } else {
                navMenu.classList.add('hidden'); navMenu.classList.remove('show');
                navMenu.style.display = 'none'; navToggle.classList.add('collapsed')
            }
        })
    }

    const dropdownLinks = document.querySelectorAll('.dropdown-toggle, .kjb-dropdown-toggle, [data-toggle="dropdown"]')
    dropdownLinks.forEach(link => {
        link.addEventListener('click', function(e) {
            e.preventDefault(); e.stopPropagation()
            const menu = this.nextElementSibling
            if (menu) {
                const isShowing = menu.classList.contains('show')
                document.querySelectorAll('.dropdown-menu, .kjb-dropdown-menu').forEach(m => {
                    m.classList.remove('show'); m.style.display = 'none'
                })
                if (!isShowing) {
                    menu.classList.add('show'); menu.style.display = 'block'
                }
            }
        })
    })
});

window.addEventListener('resize', checkBioTruncation);
</script>
