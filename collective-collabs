    <script>
    document.addEventListener('DOMContentLoaded', function() {
      const navToggle = document.querySelector('.navbar-toggler, .hamburger, .kjb-navbar-toggle, [data-toggle="collapse"]')
      const navMenu = document.querySelector('.navbar-collapse, .nav-menu, .kjb-navbar-collapse')

      if (navToggle && navMenu) {
        navToggle.addEventListener('click', function(e) {
          e.preventDefault()
          const isHidden = navMenu.classList.contains('hidden') || 
                           window.getComputedStyle(navMenu).display === 'none' ||
                           !navMenu.classList.contains('show')

          if (isHidden) {
            navMenu.classList.remove('hidden')
            navMenu.classList.add('show')
            navMenu.style.display = 'block'
            navToggle.classList.remove('collapsed')
          } else {
            navMenu.classList.add('hidden')
            navMenu.classList.remove('show')
            navMenu.style.display = 'none'
            navToggle.classList.add('collapsed')
          }
        })
      }

      const dropdownLinks = document.querySelectorAll('.dropdown-toggle, .kjb-dropdown-toggle, [data-toggle="dropdown"]')
      dropdownLinks.forEach(link => {
        link.addEventListener('click', function(e) {
          e.preventDefault()
          e.stopPropagation()
          const menu = this.nextElementSibling
          if (menu) {
            const isShowing = menu.classList.contains('show')
            document.querySelectorAll('.dropdown-menu, .kjb-dropdown-menu').forEach(m => {
              m.classList.remove('show')
              m.style.display = 'none'
            })
            if (!isShowing) {
              menu.classList.add('show')
              menu.style.display = 'block'
            }
          }
        })
      })

      document.addEventListener('click', function(e) {
        if (!e.target.closest('.dropdown, .kjb-dropdown')) {
          document.querySelectorAll('.dropdown-menu, .kjb-dropdown-menu').forEach(menu => {
            menu.classList.remove('show')
            menu.style.display = 'none'
          })
        }
      })
    })
    </script>

    <script>
        lucide.createIcons()
    </script>
</body>
</html>
