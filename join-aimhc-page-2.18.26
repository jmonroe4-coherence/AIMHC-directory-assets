                <div class="w-24 h-1 bg-sand mx-auto"></div>
            </div>

            <div class="grid grid-cols-1 md:grid-cols-3 gap-12">
                <!-- Step 1 -->
                <div class="relative">
                    <div class="text-6xl font-serif text-sand/20 absolute -top-10 -left-4 z-0">01</div>
                    <div class="relative z-10">
