    <title>Join the Collective | AI Mental Health Collective</title>
    
    <script src="https://cdn.tailwindcss.com"></script>
    <script src="https://unpkg.com/lucide@latest"></script>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600&family=Playfair+Display:ital,wght@0,600;0,700;1,600&display=swap" rel="stylesheet">
<style>
    /* Keeps site mobile menu on top of custom content */
    .navbar-collapse, .nav-menu, .kjb-navbar-collapse, .navbar {
        z-index: 9999 !important;
    }
    
    /* Prevents the custom hero from acting like a sticky site header */
    #custom-hero-section {
        position: relative !important;
        top: auto !important;
        z-index: 10 !important;
    }
</style>
    <script>
        tailwind.config = {
          theme: {
            extend: {
              fontFamily: {
                sans: ['Inter', 'sans-serif'],
                serif: ['Playfair Display', 'serif'],
              },
              colors: {
                  sage: '#6B7A6E',
                  sand: '#C9AD8D',
                  bone: '#FDFDFC',
                  charcoal: '#333333'
              }
            }
          }
        }
      </script>
</head>
<body class="bg-bone text-charcoal antialiased font-sans flex flex-col min-h-screen">

    <!-- Header: Full Bleed White, Centered Content -->
    <div id="custom-hero-section" class="bg-white border-b border-gray-100 w-full pt-0">
        <div class="max-w-7xl mx-auto py-16 md:py-24 px-6 lg:px-12 text-center">
            <div class="max-w-3xl mx-auto">
